  <!--===== ASh-12 =====-->
  <!--===== (_____) Similar to battle rifle less range more dmg =====-->
  <ThingDef ParentName="BaseBullet">
    <defName>GN_Bullet_AShTwelve</defName>
    <label>ASh-12 bullet</label>
    <graphicData>
      <texPath>Things/Projectile/Bullet_Big</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <projectile>
      <damageDef>Bullet</damageDef>
      <damageAmountBase>16</damageAmountBase>
      <speed>50</speed>
      <stoppingPower>3</stoppingPower>
    </projectile>
  </ThingDef>

  <ThingDef ParentName="GN2_BaseHumanMakeableGun">
    <defName>GN_AShTwelve</defName>
    <label>ASh-12 "Bear"</label>
    <description>Subsonic Russian battle rifle made to have high stopping power. This ShAK-12 includes a cobra sight, a laser aiming system and a sturdy foregrip.</description>
    <graphicData>
      <texPath>Things/Weapons/GN_AShTwelve</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.1</drawSize>
    </graphicData>
    <equippedAngleOffset>45</equippedAngleOffset>
    <comps>
      <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
        <eastOffset>(0.05,0,0)</eastOffset>
        <westOffset>(-0.05,0,0)</westOffset>
      </li>
    </comps>
    <soundInteract>Interact_Rifle</soundInteract>
    <recipeMaker>
      <recipeUsers>
        <li>TableMachining</li>
      </recipeUsers>
      <researchPrerequisite>GN_Research</researchPrerequisite>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
    </recipeMaker>
    <statBases>
      <WorkToMake>42000</WorkToMake>
      <Mass>3.5</Mass>
      <AccuracyTouch>0.60</AccuracyTouch>
      <AccuracyShort>0.70</AccuracyShort>
      <AccuracyMedium>0.77</AccuracyMedium>
      <AccuracyLong>0.61</AccuracyLong>
      <RangedWeapon_Cooldown>1.6</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>65</Steel>
      <Chemfuel>10</Chemfuel>
      <ComponentIndustrial>7</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>GN_Bullet_AShTwelve</defaultProjectile>
        <warmupTime>1.2</warmupTime>
        <range>34</range>
        <burstShotCount>2</burstShotCount>
        <ticksBetweenBurstShots>10</ticksBetweenBurstShots>
        <soundCast>GN_Shot_AShTwelve</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>12</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>

